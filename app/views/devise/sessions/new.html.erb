<h2>Log in</h2>
<div id="form-wrapper">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <div<%= f.label :email %><br /></div>
    <div><%= f.email_field :email, autofocus: true, autocomplete: "email" %></div>
  </div>

  <div class="field">
    <div><%= f.label :password %><br /></div>
    <div><%= f.password_field :password, autocomplete: "off" %></div>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <div><%= f.check_box :remember_me %></div>
      <div><%= f.label :remember_me %></div>
    </div>
  <% end -%>

  <div class="actions">
    <div><%= f.submit "Log in" %></div>
  </div>
</div>
<% end %>


<%= render "devise/shared/links" %>
